{
  "name": "weather-dashboard-dark",
  "version": "2.0.0",
  "description": "Modern weather dashboard with dark mode and theme toggle",
  "main": "run_server.py",
  "scripts": {
    "start": "python run_server.py",
    "dev": "uvicorn run_server:app --reload --host 0.0.0.0 --port 8000",
